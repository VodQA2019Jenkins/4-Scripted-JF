node('Master') {

	checkout scm
	
	stage 'Maven Clean Compile Package Install Test'
    bat 'mvn -f spring-boot-samples\\spring-boot-sample-atmosphere\\pom.xml clean compile package install test'

	stage 'Archive Artifacts'
    archiveArtifacts artifacts: 'spring-boot-samples\\spring-boot-sample-atmosphere\\target\\*.jar'
          
}